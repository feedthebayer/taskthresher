<section id="user-form">
  <div class="row">
    <header class="content">
      <h1>Free Registration</h1>
    </header>
  </div>

  <%= form_for @user do |form| %>
    <%= render partial: '/users/form', object: form %>
    <div class="row">
      <div class="small-12 medium-7 large-5 small-centered columns">
        <%= form.submit "Register", class: "register button expand" %>
      </div>
    </div>
  <% end %>
</section>
